        "guzzlehttp/guzzle": "~6.5",
        "firebase/php-jwt": "~5.2",
        "ext-json": "*",
        "ext-openssl": "*"
    },
    "require-dev": {
        "mockery/mockery": "1.3.1",
        "phpunit/phpunit": "5.7.27"
    },
    "autoload": {
        "psr-4": {
            "CurrencyFair\\AppleId\\": "src"
        }
